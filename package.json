    "@angular/compiler-cli": "^11.2.14",
    "@types/jasmine": "~3.5.0",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "^12.12.53",
    "codelyzer": "^5.1.0",
    "jasmine-core": "~3.5.0",
