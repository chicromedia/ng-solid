  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "build:core": "ng-packagr -p packages/core/ng-package.json",
    "build:components": "ng-packagr -p packages/components/ng-package.json -c packages/components/tsconfig.lib.prod.json",
    "build:facebook": "ng-packagr -p packages/facebook/ng-package.json",
    "build:twitter": "ng-packagr -p packages/twitter/ng-package.json",
    "build:google": "ng-packagr -p packages/google/ng-package.json",
    "publish:core": "npm publish ./dist/core --access=public",
    "publish:components": "npm publish ./dist/components --access=public",
    "publish:facebook": "npm publish ./dist/facebook --access=public",
    "publish:twitter": "npm publish ./dist/twitter --access=public",
    "publish:google": "npm publish ./dist/google --access=public",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e"
  },
  "private": false,
  "license": "MIT",
